    <script>

        Array.prototype.contains = function(v) {
            for(var i = 0; i < this.length; i++) {
                if(this[i] === v) return true;
            }
            return false;
        };

        var offices =null;
        

        $(document).ready(function() {
            offices = new Array();
            $.each($(offices, function (i, item){
            if(!offices.contains(item.petType)){
                offices.push(item.petType);
            }
            });

           
            var table = $('#clients').DataTable( {
                columnDefs:[{targets:[0,1,3,4,5], type:"dom-text", render:function(data, type, row, meta){
            return "<input type='text' value='" +  data + "'>";}},
            {targets:[2], render : 
         function(data){return createSelect(data);}}           
                   ]
            });
            "scrollY": "420px",
            "paging": true
        });

        

        function createSelect(selItem){
            var sel = "<select><option>Select City</option>" ;
            for(var i = 0; i < offices.length; ++i){
                if(offices[i] == selItem){
                    sel += "<option selected value = '"+offices[0]+"' >" + offices[i] + "</option>";
                }
                else{
                    sel += "<option  value = '"+offices[0]+"' >" + offices[i] + "</option>";
                }
            }
            sel += "</select>";
            return sel;
        }

            $('a.toggle-vis').on( 'click', function (e) {
                e.preventDefault();

            // Get the column API object
            var column = table.column( $(this).attr('data-column') );

            // Toggle the visibility
            column.visible( ! column.visible() );
            } );
        } );
    </script>


